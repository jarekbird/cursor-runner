{
  "mcpServers": {
    "cursor-runner-shared-sqlite": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-sqlite-npx",
        "/app/shared_db/shared.sqlite3"
      ]
    },
    "cursor-agents": {
      "command": "sh",
      "args": [
        "-c",
        "cd /app/target/cursor-agents && node dist/mcp/index.js"
      ],
      "env": {
        "REDIS_URL": "redis://redis:6379/0",
        "NODE_ENV": "production"
      }
    }
  }
}

