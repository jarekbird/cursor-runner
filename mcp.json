{
  "mcpServers": {
    "cursor-runner-shared-sqlite": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-sqlite-npx",
        "/app/shared_db/shared.sqlite3"
      ]
    },
    "cursor-runner-shared-redis": {
      "command": "npx",
      "args": [
        "-y",
        "@liangshanli/mcp-server-redis",
        "--url",
        "redis://redis:6379/0"
      ],
      "env": {
        "REDIS_URL": "redis://redis:6379/0"
      }
    }
  }
}

